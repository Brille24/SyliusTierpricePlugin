{
    "name": "brille24/sylius-tierprice-plugin",
    "type": "sylius-plugin",
    "description": "A plugin that allows to add tierprices to Sylius",
    "license": "MIT",
    "require": {
        "php": "^7.2",

        "sylius/sylius": "^1.9",
        "friendsofsymfony/oauth-server-bundle": "2.0.x-dev"
    },
    "require-dev": {
        "behat/behat": "^3.6.1",
        "behat/mink-selenium2-driver": "^1.3",
        "friends-of-behat/mink-extension": "^2.4",
        "friends-of-behat/variadic-extension": "^1.3",
        "friends-of-behat/page-object-extension": "^0.3",
        "friends-of-behat/symfony-extension": "^2.1",
        "friends-of-behat/mink": "^1.8",
        "friends-of-behat/mink-browserkit-driver": "^1.4",
        "lakion/mink-debug-extension": "^2.0.0",
        "phpspec/phpspec": "^5.0|^6.1",
        "phpstan/phpstan": "^0.12",
        "phpstan/phpstan-doctrine": "^0.12",
        "phpstan/phpstan-strict-rules": "^0.12",
        "phpstan/phpstan-symfony": "^0.12",
        "phpstan/phpstan-webmozart-assert": "^0.12",
        "phpunit/phpunit": "^9.5",
        "sensiolabs/security-checker": "^6.0",
        "sylius-labs/coding-standard": "^3.1",
        "symfony/browser-kit": "^4.4",
        "symfony/debug-bundle": "^4.4|^5.0",
        "symfony/dotenv": "^4.4|^5.0",
        "symfony/intl": "^4.4|^5.0",
        "symfony/web-profiler-bundle": "^4.4|^5.0",
        "symfony/web-server-bundle": "^4.4|^5.0",
        "friends-of-behat/suite-settings-extension": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Brille24\\SyliusTierPricePlugin\\": "src/",
            "Tests\\Brille24\\SyliusTierPricePlugin\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}

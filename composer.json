{
    "name": "brille24/sylius-tierprice-plugin",
    "type": "sylius-plugin",
    "description": "A plugin that allows to add tierprices to Sylius",
    "license": "MIT",
    "require": {
        "php": "^7.3",

        "sylius/sylius": "^1.8"
    },
    "require-dev": {
        "behat/behat": "^3.4",
        "behat/mink": "^1.7@dev",
        "behat/mink-browserkit-driver": "^1.3",
        "behat/mink-extension": "^2.2",
        "behat/mink-selenium2-driver": "^1.3",
        "friends-of-behat/variadic-extension": "^1.1",
        "friends-of-behat/page-object-extension": "^0.3",
        "friends-of-behat/symfony-extension": "^2.0",
        "lakion/mink-debug-extension": "^1.2.3",
        "phpspec/phpspec": "^5.0|^6.1",
        "phpunit/phpunit": "^6.5|^8.0",
        "sensiolabs/security-checker": "^5.0",
        "sylius-labs/coding-standard": "^2.0|^3.1",
        "symfony/browser-kit": "^4.1",
        "symfony/debug-bundle": "^4.1",
        "symfony/dotenv": "^4.1",
        "symfony/intl": "^4.1",
        "symfony/web-profiler-bundle": "^4.1",
        "symfony/web-server-bundle": "^4.1",
        "friends-of-behat/suite-settings-extension": "^1.0"
    },
    "conflict": {
        "symfony/symfony": "4.1.8",
        "symfony/browser-kit": "4.1.8",
        "symfony/dependency-injection": "4.1.8",
        "symfony/dom-crawler": "4.1.8",
        "symfony/routing": "4.1.8"
    },
    "autoload": {
        "psr-4": {
            "Brille24\\SyliusTierPricePlugin\\": "src/",
            "Tests\\Brille24\\SyliusTierPricePlugin\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
